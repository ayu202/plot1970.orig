FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

D 0;
Fc 1.0;
F 0.3;

//kansen
porosity0
{
    type            explicitPorositySource;
    active          true;
    selectionMode   cellZone;
    cellZone        line0;

    explicitPorositySourceCoeffs
    {
        selectionMode   cellZone;
        
        cellZone        line0;
        
        type            DarcyForchheimer;

        d   ($D $D $D);
        f   ($F $F $F);

        coordinateSystem
        {
            origin  (0 0 0);

            rotation
            {
                type    none;
            }
        }
    }
}

